<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/labo_mission_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/labo_mission_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/labo_mission_name"/>
    <TextView
        android:id="@+id/labo_mission_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/labo_mission_start"/>
    <TextView
        android:id="@+id/labo_mission_file"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/labo_mission_text"
        android:clickable="true"/>


    <TextView
        android:id="@+id/labo_mission_finish_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/labo_mission_file"/>
    <TextView
        android:id="@+id/labo_mission_finish_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/labo_mission_finish_time"/>

</RelativeLayout>